    "    answer = ''\n",
    "    data = []\n",
    "    \n",
    "    data = list(''.join(str(n)))\n",
    "    data.sort(reverse=True)\n",
    "    \n",
    "    for i in data:\n",
    "        answer += i\n",
    "    \n",
    "    answer = int(answer)\n",
    "    \n",
    "    return answer"
