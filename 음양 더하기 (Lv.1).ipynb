   "source": [
    "def solution(absolutes, signs):\n",
    "    answer = 0\n",
    "    \n",
    "    for i in range(len(absolutes)):\n",
    "        if signs[i]:\n",
    "            answer += absolutes[i]\n",
    "            \n",
    "        else:\n",
    "            answer += - (absolutes[i])\n",
    "\n",
    "    return answer"
   ]
