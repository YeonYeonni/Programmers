   "source": [
    "def solution(phone_number):\n",
    "    answer = ''\n",
    "    data = []\n",
    "    \n",
    "    data = list(''.join(phone_number))\n",
    "    \n",
    "    for i in range(0, len(data) - 4):\n",
    "        answer += '*'\n",
    "    \n",
    "    for j in range(len(data) - 4, len(data)):\n",
    "        answer += data[j]\n",
    "    \n",
    "    return answer"
   ]
